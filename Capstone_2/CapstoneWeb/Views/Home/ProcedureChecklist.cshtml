
@{
    ViewBag.Title = "ProcedureChecklist";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}
<br />
<br/>
<div class="row">
    <div class="col-md-offset-3 col-md-4">
        <h2>Procedure & Checklist</h2>
        <br/>
        <table>
            <tr>
                <th>Sl.No</th>
                <th>Title</th>
                <th>Documents</th>
            </tr>
            <tr><td>1</td>
                <td>New Application</td>
                <td>@Html.ActionLink("New Application","Download","Home",new{name="NewApplication" },new {target="_blank" })</td>
            </tr>
