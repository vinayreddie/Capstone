<script>
    $(document).ready(function () {
        $("#fsNewApplication").hide();
        $("#fsApplicationwithSLA").hide();
        $("#fsApplicationExceedingSLA").hide();
        $("#fsApplicationPending").hide(); 
        $("#fsApplicationAppeal").hide();
        $("#fsFarword").hide();
        $("#fsReturnApplication").hide();

    });
    $(aResubmit).click(function () {
        $("#fsNewApplication").hide();
        $("#fsApplicationwithSLA").hide();
        $("#fsApplicationExceedingSLA").hide();
        $("#fsApplicationPending-Stage").hide();
        $("#fsApplicationAppeal").hide();
        $("#fsFarword").hide();
        $("#fsReturnApplication").hide();
        $("#fsResubmit").show();
    });
   
    $("#aTotalApplications").click(function () {       
        $("#fsNewApplication").show();
